    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.2",
    "@eslint/js": "^9.39.1",
    "@tsconfig/node22": "^22.0.5",
